{
  "name": "documentcloud-frontend",
  "version": "1.0.0",
  "type": "module",
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@sentry/svelte": "^7.66.0",
    "@sentry/vite-plugin": "^2.10.2",
    "@tsconfig/svelte": "^2.0.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "autoprefixer": "^10.4.14",
    "axios": "1.5.1",
    "axios-retry": "^3.5.1",
    "body-scroll-lock": "^2.6.4",
    "concurrently": "^5.3.0",
    "cross-env": "^5.2.0",
    "dompurify": "^3.0.5",
    "dotenv": "^16.0.3",
    "fast-copy": "^2.1.0",
    "fast-deep-equal": "^3.1.3",
    "lucene": "^2.1.1",
    "marked": "^7.0.5",
    "marked-gfm-heading-id": "^3.0.6",
    "postcss": "^8.4.25",
    "rlite-router": "^2.0.3",
    "sass": "^1.60.0",
    "scrollzoom": "MuckRock/scrollzoom",
    "svelte": "^4.0.0",
    "svelte-i18n": "^4.0.0",
    "svelte-octicons": "^18.4.0",
    "svelte-preprocess": "^5.0.3",
    "svelte-preprocess-sass": "^2.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^5.0.3",
    "vite": "^4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-svelte-csf": "^4.0.12",
    "@storybook/blocks": "^7.5.3",
    "@storybook/svelte": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "eslint": "^7.32.0",
    "msw": "^1.2.3",
    "msw-storybook-addon": "^1.10.0",
    "playwright": "^1.39.0",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^14.2.0",
    "storybook": "7.5.3",
    "tape": "^5.7.2",
    "vitest": "^0.34.6"
  },
  "scripts": {
    "build": "vite build",
    "build-staging": "cross-env NODE_ENV=staging vite build",
    "build-serve": "cross-env NODE_ENV=production vite preview",
    "build-storybook": "storybook build",
    "dev": "vite --host 0.0.0.0",
    "dev-app": "cross-env NODE_ENV=development SUPPRESS_WARNINGS=1 webpack serve --config webpack.app.config.js",
    "dev-embed": "cross-env NODE_ENV=development webpack --config webpack.embed.config.js",
    "preview": "vite preview",
    "serve": "serve public -l 80 --single",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test-watch": "vitest",
    "test:browser": "playwright test",
    "watch": "concurrently npm:serve npm:dev-embed npm:watch-app",
    "watch-app": "cross-env NODE_ENV=development webpack watch --config webpack.app.config.js"
  },
  "engine": 18,
  "browserslist": "> 0.25%, not dead",
  "msw": {
    "workerDirectory": "public"
  }
}
